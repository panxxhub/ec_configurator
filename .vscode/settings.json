{
	"editor.fontFamily": "'Cascadia Code', 'Fira Code Retina', 'Jetbrains Mono', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 17,
	"editor.linkedEditing": true,
	"editor.lightbulb.enabled": true,
	"editor.formatOnSave": true,
	"editor.stickyScroll.enabled": true,
	"protoc": {
		"path": "${workspaceRoot}/protos"
	},
	"clangd.checkUpdates": false,
	"clangd.serverCompletionRanking": true,
	"clangd.path": "/usr/bin/clangd-15",
	"clangd.arguments": [
		"--compile-commands-dir=${workspaceFolder}/build",
		"--pch-storage=memory",
		"--clang-tidy=true",
		"--all-scopes-completion=true",
		"--background-index=true",
		"--inlay-hints=true",
		"--pretty",
	],
	"editor.semanticHighlighting.enabled": true,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "boundary",
	"editor.minimap.renderCharacters": true,
	"terminal.integrated.fontFamily": "Hasklug NF",
	"editor.semanticTokenColorCustomizations": {
		"rules": {
			"enum": {
				"fontStyle": "bold"
			},
			"enumMember": {
				"foreground": "#a0cd00",
				"fontStyle": "italic bold"
			},
			"macro": {
				"foreground": "#2cac61",
				"fontStyle": "bold underline"
			},
			"function": {
				"fontStyle": "bold"
			},
			"variable": {
				"fontStyle": "italic"
			},
			"property": {
				"fontStyle": "italic underline",
				"foreground": "#c943e1"
			}
		}
	},
	"workbench.colorTheme": "Bluloco Light",
	"oneDarkPro.vivid": true,
	"oneDarkPro.bold": true,
	"files.associations": {
		"array": "c",
		"*.tcc": "c",
		"bitset": "c",
		"functional": "c",
		"mutex": "c",
		"tuple": "c",
		"variant": "c",
		"chrono": "c",
		"__config": "c",
		"__tuple": "c",
		"ios": "c",
		"span": "c",
		"string": "c",
		"string_view": "c",
		"vector": "c",
		"kernel.h": "c",
		"init.h": "c",
		"module.h": "c",
		"virtio_dma_buf.h": "c",
		"virtio_mmio.h": "c",
		"virtio_input.h": "c",
		"virtio_gpio.h": "c",
		"virtio_console.h": "c",
		"virtio.h": "c",
		"virtio_i2c.h": "c",
		"vringh.h": "c",
		"virtio_ring.h": "c",
		"virtio_config.h": "c",
		"domain.h": "c",
		"globals.h": "c",
		"master.h": "c"
	},
	"cSpell.enabled": true,
	"cSpell.words": [
		"cpuset",
		"ecrt",
		"PDOS",
		"SCARA",
		"setaffinity",
		"tinyfsm",
		"traj"
	]
}